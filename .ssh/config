ServerAliveInterval 60

# HashKnownHosts can go bite my shiny metal ass! I want tab-completion to
# work, you stupid cunts!
HashKnownHosts no

Host *
    ControlMaster auto
    ControlPath ~/.ssh/master-%r@%h:%p

Host parabox.dk
    User sune
    ForwardAgent yes

Host brig.ncc-1701.dk
    User sune
    Port 2222
    ForwardAgent yes

Host *.diku.dk
    User kirkeby

Host *.ibofobi.dk
    User sune
    ForwardAgent yes
    #ControlMaster auto

Host *.vbox-nat.ibofobi.dk
    Hostname localhost
    Port 2222
    User sune
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    LocalForward 8000 127.0.0.1:8000

Host *.csis.local
    User ski
    IdentityFile id_dsa.csis
    ForwardAgent yes
Host *.csis.internal
    User ski
    IdentityFile id_dsa.csis
    ForwardAgent yes
Host *.csis.dk
    User ski
    IdentityFile id_dsa.csis
    ForwardAgent yes

Host sshgw.ecrime.csis.internal
    ProxyCommand none
    DynamicForward 11223

Host eihttp01.ecrime.csis.internal
    LocalForward 4343 127.0.0.1:443

Host *.ecrime.csis.internal
