ServerAliveInterval 60

# HashKnownHosts can go bite my shiny metal ass! I want tab-completion to
# work, you stupid cunts!
HashKnownHosts no

Host *
    ControlMaster auto
    ControlPath ~/.ssh/master-%r@%h:%p

Host parabox.dk
    User sune
    ForwardAgent yes

Host brig.ncc-1701.dk
    User sune
    Port 2222
    ForwardAgent yes

Host atlas.munintech.dk
    Port 8888

Host *.diku.dk
    User kirkeby

Host *.ibofobi.dk
    User sune
    ForwardAgent yes
    #ControlMaster auto

Host *.vbox-nat.ibofobi.dk
    Hostname localhost
    Port 2222
    User sune
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    LocalForward 8000 127.0.0.1:8000

Host *.csis.local
    User ski
    IdentityFile ~/Private/csis/ssh/id_dsa
    ForwardAgent yes
Host *.csis.internal
    User ski
    IdentityFile ~/Private/csis/ssh/id_dsa
    ForwardAgent yes
Host *.csis.dk
    User ski
    IdentityFile ~/Private/csis/ssh/id_dsa
    ForwardAgent yes

Host sshgw.ecrime.csis.internal
    ProxyCommand none
    DynamicForward 11223

